{
    "$schema": "vscode://schemas/color-theme",
    "name": "Markdown Support",
    "tokenColors": [
                {
            "scope": "punctuation.definition.quote.begin.markdown",
            "settings": {
                "foreground": "#4a6b3b"
            }
        },
        {
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#3e5d91"
            }
        },
        {
            "name": "Markdown - Plain",
            "scope": [
                "text.html.markdown",
                "punctuation.definition.list_item.markdown"
            ],
            "settings": {
                "foreground": "#212121"
            }
        },
        {
            "name": "Markdown - Markup Raw Inline",
            "scope": [
                "text.html.markdown markup.inline.raw.markdown"
            ],
            "settings": {
                "foreground": "#1660c2"
            }
        },
        {
            "name": "Markdown - Markup Raw Inline Punctuation",
            "scope": [
                "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
            ],
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "name": "Markdown - Line Break",
            "scope": [
                "text.html.markdown meta.dummy.line-break"
            ],
            "settings": {
                "foreground": ""
            }
        },
        {
            "name": "Markdown - Heading",
            "scope": [
                "markdown.heading",
                "markup.heading | markup.heading entity.name",
                "markup.heading.markdown punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#5c1a92"
            }
        },
        {
            "name": "Markup - Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#232c38"
            }
        },
        // {
        //     "scope": "markup.heading",
        //     "settings": {
        //         "fontStyle": "bold",
        //         "foreground": "#800000"
        //     }
        // },
        {
            "name": "Markup - Bold",
            "scope": [
                "markup.bold",
                "markup.bold string"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#22465e"
            }
        },
        {
            "name": "Markup - Bold-Italic",
            "scope": [
                "markup.bold markup.italic",
                "markup.italic markup.bold",
                "markup.quote markup.bold",
                "markup.bold markup.italic string",
                "markup.italic markup.bold string",
                "markup.quote markup.bold string"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#22465e"
            }
        },
        {
            "name": "Markup - Underline",
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#232c38"
            }
        },
        // {
        //     "name": "Markup - Strike",
        //     "scope": [
        //         "markup.strike"
        //     ],
        //     "settings": {
        //         "fontStyle": "strike",
        //         "foreground": ""
        //     }
        // },
        {
            "name": "Markdown - Quote",
            "scope": [
                "markup.quote punctuation.definition.blockquote.markdown", // block quote
                "markup.quote"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#232c38"
            }
        },
        {
            "name": "Markdown - Link",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.title.markdown", // "Markdown - Link Description",
                "constant.other.reference.link.markdown" // "Markdown - Link Anchor"
            ],
            "settings": {
                "foreground": "#64da74"
            }
        },
        {
            "name": "Markdown - Fenced Bode Block Variable",
            "scope": [
                "markup.fenced_code.block.markdown",
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#EEFFFF90"
            }
        },
        {
            "name": "Markdown - Fenced Language",
            "scope": [
                "variable.language.fenced.markdown"
            ],
            "settings": {
                "foreground": "#5d7c92a4"
            }
        },
        {
            "name": "Markdown - Separator",
            "scope": [
                "meta.separator"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#262879cc"
            }
        },
        {
            "name": "Markup - Table",
            "scope": [
                "markup.table"
            ],
            "settings": {
                "foreground": "#EEFFFF"
            }
        },
        {
            "name": "Markup - Raw Block",
            "scope": [
                "markup.raw.block"
            ],
            "settings": {
                "foreground": "#9e3fe2"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#798b70"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#8f6351"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#37658a"
            }
        },

        {
            "scope": "punctuation.definition.quote.begin.markdown",
            "settings": {
                "foreground": "#5d864a"
            }
        },
        {
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#3c5886"
            }
        },
    ]
}